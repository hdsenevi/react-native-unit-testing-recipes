{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "babel",
        "react"
    ],
    "env": {
        "browser": true,
        "mocha": true,
        "node": true,
        "jest": true
    },
    "globals": {
        "__DEV__": true,
        "React$Element": true
    },
    "rules": {
        "arrow-body-style": 0,
        "arrow-parens": [
            2,
            "always"
        ],
        "import/extensions": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "import/no-named-as-default": 0,
        "import/prefer-default-export": 0,
        "jsx-boolean-value": 0,
        "no-floating-decimal": 0,
        "radix": [
            "error",
            "as-needed"
        ],
        "react/jsx-filename-extension": 0,
        "react/sort-comp": 0,
        "no-console": [
            "error",
            {
                "allow": [
                    "warn",
                    "error"
                ]
            }
        ],
        "global-require": 0,
        "max-len": 0,
        "class-methods-use-this": 0,
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 1,
                "maxBOF": 0,
                "maxEOF": 1
            }
        ],
        "react/prop-types": "off"
    }
}